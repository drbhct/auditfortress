{
  "name": "auditfortress",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "build:performance": "vite build --config vite.config.performance.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "perf:analyze": "node scripts/analyze-bundle.js",
    "perf:monitor": "npm run dev -- --open",
    "perf:lighthouse": "lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html",
    "perf:audit": "npm run build:analyze && npm run perf:lighthouse"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@heroicons/react": "^2.1.5",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.59.20",
    "@tiptap/extension-bullet-list": "^3.4.5",
    "@tiptap/extension-color": "^3.4.5",
    "@tiptap/extension-link": "^3.4.5",
    "@tiptap/extension-list-item": "^3.4.5",
    "@tiptap/extension-ordered-list": "^3.4.5",
    "@tiptap/extension-placeholder": "^3.4.5",
    "@tiptap/extension-table": "^3.4.5",
    "@tiptap/extension-table-cell": "^3.4.5",
    "@tiptap/extension-table-header": "^3.4.5",
    "@tiptap/extension-table-row": "^3.4.5",
    "@tiptap/extension-text-style": "^3.4.5",
    "@tiptap/react": "^3.4.5",
    "@tiptap/starter-kit": "^3.4.5",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^6.28.0",
    "react-window": "^2.1.1",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^5.4.10",
    "vitest": "^3.2.4"
  }
}
